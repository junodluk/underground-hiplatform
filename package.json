{
  "name": "underground-hiplatform",
  "version": "1.0.0",
  "description": "",
  "author": "Alcione de Lucca JÃºnior",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "start:local": "node scripts/start.js local",
    "build": "node scripts/build.js",
    "commit": "git-cz",
    "test": "node scripts/test.js",
    "eslint": "node node_modules/eslint/bin/eslint ./",
    "eslint:fix-indent": "node node_modules/eslint/bin/eslint --fix --ext js --no-eslintrc  --rule 'indent: [error,2,{SwitchCase: 1}]' src/**/*.js",
    "eslint:fix-all": "node node_modules/eslint/bin/eslint --fix src/**/*.js"
  },
  "dependencies": {
    "@babel/core": "7.1.0",
    "@svgr/webpack": "2.4.1",
    "axios": "^0.18.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.2",
    "babel-preset-react-app": "^5.0.4",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest-enzyme": "^7.0.0",
    "mini-css-extract-plugin": "0.4.3",
    "node-sass": "^4.9.4",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "querystring": "^0.2.0",
    "react": "^16.6.0",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.0.5",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webfontloader": "^1.6.28",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.2"
  },
  "devDependencies": {
    "babel-eslint": "9.0.0",
    "babel-jest": "^23.6.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-react": "7.11.1",
    "jest": "^23.6.0",
    "jest-pnp-resolver": "^1.0.1",
    "jest-resolve": "^23.6.0",
    "react-md": "^1.8.0",
    "react-test-renderer": "^16.6.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junodluk/underground-hiplatform.git"
  },
  "bugs": {
    "url": "https://github.com/junodluk/underground-hiplatform/issues"
  },
  "homepage": "https://github.com/junodluk/underground-hiplatform#readme",
  "config": {
    "commitizen": {
      "path": "./cz"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*.{js,jsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost:6000",
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/setup-tests.js",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "verbose": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
